@model float
@{
    ViewBag.Title = "Account History";
}

<div class="row">
    <div class="col-md-8 col-md-offset-2">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <h3 class="panel-title heading-bold">Automated Teller Machine</h3>
            </div>
            <div class="panel-body dark-font">
                <h2>Account History</h2>
                <div class="row">
                    <div class="col-sm-3">
                        <ul class="nav nav-pills nav-stacked">
                            <li role="presentation" class="active"><a href="#">Example Account <span class="badge">$@Math.Round(Model, 2).ToString()</span></a></li>
                        </ul>
                    </div>
                    <div class="col-sm-9">
                        <table class="table table-striped">
                            <thead>
                                <tr>
                                    <th>Date</th>
                                    <th>Description</th>
                                    <th>Amount</th>
                                    <th>Account Balance</th>
                                </tr>
                            </thead>
                        </table>
                        <div class="alert alert-info"><strong>NYI: Account History</strong></div>
                    </div>
                </div>
                
            </div>
        </div>
    </div>
</div>
